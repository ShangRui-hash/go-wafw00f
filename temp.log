r\n                var list = data.result;\r\n                var html = '';\r\n                for (var i = 0; i < list.length; i++) {\r\n                    var info = list[i]\r\n                    var timeInfo = info[2];\r\n                    var year = timeInfo.year.toString();\r\n                    year = year.substr(1,2);\r\n                    html += '<div class=\"part04_newtit\"><div class=\"pic_part04date\">'+ timeInfo.date + ' 日</div>';\r\n                    html +=     '<div class=\"part04_ctmid\">';\r\n                    html +=         '<dt>'+ year+ '年'+ (timeInfo.month + 1) + '月' +'</dt>';\r\n                    html +=         '<dd>' + info[0] + '</dd>';\r\n                    html +=     '</div>';\r\n                    html +=    '<div class=\"part04_jjh\"><a rel=\"nofollow\" href=\"'+info[1]+'\"target=\"_blank\" >></a></div>';\r\n                    html +=  '</div>\\n';\r\n                }\r\n                $(\"#zxzx_xwzx_id\").append(html);\r\n            }\r\n        });\r\n    }\r\n\r\n    //获取最新的五条技术研究\r\n    function getLatest5News(){\r\n        $.ajax({\r\n            url: \"/getLatest5News.html?_t_=\" + Math.random(),\r\n            dataType: \"json\",\r\n            type: \"post\",\r\n            data: {},\r\n            success: function (data) {\r\n                var list = data.result;\r\n                var html = '';\r\n                for (var i = 0; i < list.length; i++) {\r\n                    var info = list[i]\r\n                    var timeInfo = info.time;\r\n                    var year = timeInfo.year.toString();\r\n                    year = year.substr(1,2);\r\n                    html += '<div class=\"part04_newtit\"><div class=\"pic_part04date\">'+ timeInfo.date + ' 日</div>';\r\n                    html +=     '<div class=\"part04_ctmid\">';\r\n                    html +=         '<dt>'+year+ '年'+ (timeInfo.month + 1) + '月' +'</dt>';\r\n                    html +=         '<dd>' + info.title + '</dd>';\r\n                    html +=     '</div>';\r\n                    \r\n                    html +=    '<div class=\"part04_jjh\"><a rel=\"nofollow\" href=\"/news.html?id='+info.id+' \"target=\"_blank\" >></a></div>';\r\n\r\n\r\n                    html +=  '</div>\\n';\r\n                }\r\n                $(\"#zxzx_jsyj_id\").append(html);\r\n            }\r\n        });\r\n    }\r\n\r\n    $(function () {\r\n        getDynamicTop5List();\r\n        getLatest5News();\r\n    })\r\n</script>\r\n</html>\r\n\n"
